            url: "GetCategories",
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify(jdata),
            dataType: "json",
            success: function (data) {
                $.map(data, function (item) {
                    if (item.Marathi == "0")
                        $('#liMarathi').hide();
                    if (item.English == "0")
                        $('#liEnglish').hide();
                    if (item.Geopolytical == "0")
                        $('#liGeopolitical').hide();
                    //if (item.RuralAgri == "0")
