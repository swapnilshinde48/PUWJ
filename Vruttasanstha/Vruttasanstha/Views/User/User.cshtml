                    $(".modal").show();
                },
                complete: function () {
                    $(".modal").hide();
                },
                data: JSON.stringify(jsondata),
                dataType: "json",
                success: function (data) {
                    $.map(data, function (item) {
                        $('#nheading').text(item.NHeading);
                        $('#txtuphiddenid').val(item.NId);
                        $('#txtdate').text(toDateFromJson(item.NcreatedDate));
                        $('#txtauthor').text(item.NCreatedby);
                        $('#description').html(item.Ndescription);
                        var base64 = base64js.fromByteArray(item.Nimage);
                        $('#imgupload1').attr('src', "data:image/jpg;base64," + base64);
                    });
                },
                error: function (result) {
                    window.location.href = '../Home/ErrorPage';
                },
            });
        });
        var jdata = { 'id': $('#hiddenid').val() }
